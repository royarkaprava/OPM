
################################################################################################
########################  USE OF THE fit.ALASSOOPM FUNCTION ####################################
################################################################################################



load("data.rda")
phenotypes2 <- Macro.micro_2_test
data <- phenotypes2
Ymat <- data[, 1:6]
X    <- data[, -c(1:6)]
test <- 121:277
train <- 1:120
IndMa <- 20

out <- fit.ALASSOOPM(Ymat[, 2], X, LAMBDA = 0.5, ncores = 1, test, train, IndMa)

#############################################################################################
#############################################################################################



